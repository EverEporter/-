    "\n",
    "\n",
    "def process_entry(driver, entry):\n",
    "    try:\n",
    "        driver.get(\"http://139.224.101.45/report/\")\n",   #这里填入下载网址
    "        \n",
    "        input_element = WebDriverWait(driver, 10).until(\n",
    "            EC.presence_of_element_located((By.ID, \"bianhao\"))\n",
    "        )\n",
    "        input_element.clear()\n",
    "        input_element.send_keys(entry[\"link\"])\n",
    "        \n",
    "        button = WebDriverWait(driver, 10).until(\n",
    "            EC.presence_of_element_located((By.ID, \"jxReportButton\"))\n",
    "        )\n",
    "        driver.execute_script(\"arguments[0].removeAttribute('disabled');\", button)\n",
    "        button.click()\n",
    "        \n",
    "        download_link = WebDriverWait(driver, 15).until(\n",
    "            EC.presence_of_element_located((By.XPATH, '//div[@class=\"alert alert-secondary\"]//a[contains(@href, \"https://bigdata-s3.wmcloud.com/\")]'))\n",
    "        ).get_attribute(\"href\")\n",
    "        \n",
    "        return {**entry, \"download\": download_link, \"status\": \"success\"}\n",
    "    except Exception as e:\n",
    "        print(f\"处理条目失败: {entry['title']}，错误: {str(e)}\")\n",
    "        return {**entry, \"download\": None, \"status\": f\"failed: {str(e)}\"}\n",
    "\n",
    "def main():\n",
    "    with open('raw-data/策略.json', 'r', encoding='utf-8') as f:                     #更改这里的源json\n",
    "        input_list = json.load(f)\n",
    "    \n",
    "    output_path = 'add-links/策略download-links.json'                          #更改这里的outputpath\n",
    "    existing_entries = {}\n",
    "    \n",
    "    # 读取已有输出文件\n",
    "    if os.path.exists(output_path):\n",
    "        try:\n",
    "            with open(output_path, 'r', encoding='utf-8') as f:\n",
    "                output_entries = json.load(f)\n",
    "                for entry in output_entries:\n",
    "                    existing_entries[entry['link']] = entry\n",
    "        except (json.JSONDecodeError, IOError) as e:\n",
    "            print(f\"警告：输出文件读取失败，将重新生成。错误: {str(e)}\")\n",
    "    \n",
    "    # 生成待处理列表（未成功或新增条目）\n",
    "    to_process = [\n",
    "        entry for entry in input_list\n",
    "        if entry['link'] not in existing_entries \n",
    "        or existing_entries[entry['link']].get('status') != 'success'\n",
    "    ]\n",
    "    \n",
    "    if not to_process:\n",
    "        print(\"所有条目均已成功处理，无需重复操作。\")\n",
    "        return\n",
    "    \n",
    "    # 初始化浏览器\n",
    "    options = webdriver.ChromeOptions()\n",
    "    options.add_argument(\"--disable-gpu\")\n",
    "    options.add_argument(\"--no-sandbox\")\n",
    "    driver = webdriver.Chrome(options=options)\n",
    "    \n",
    "    try:\n",
    "        results = []\n",
    "        for idx, entry in enumerate(to_process, 1):\n",
    "            print(f\"正在处理差异条目 {idx}/{len(to_process)}: {entry['title']}\")\n",
    "            result = process_entry(driver, entry)\n",
    "            results.append(result)\n",
    "            time.sleep(0.5)\n",
    "        \n",
    "        # 合并处理结果到现有数据\n",
    "        for result in results:\n",
    "            existing_entries[result['link']] = result\n",
    "        \n",
    "        # 按原始输入顺序生成最终输出\n",
    "        final_output = []\n",
    "        for input_entry in input_list:\n",
    "            final_entry = existing_entries.get(input_entry['link'], input_entry)\n",
    "            final_output.append(final_entry)\n",
    "        \n",
    "        # 写入完整数据（覆盖模式）\n",
    "        with open(output_path, 'w', encoding='utf-8') as f:\n",
    "            json.dump(final_output, f, ensure_ascii=False, indent=2)\n",
    "        \n",
    "        success_count = sum(1 for entry in final_output if entry.get('status') == 'success')\n",
    "        print(f\"处理完成！成功数: {success_count}/{len(final_output)}\")\n",
    "    \n",
    "    except Exception as e:\n",
    "        print(f\"全局错误: {str(e)}\")\n",
    "    finally:\n",
    "        driver.quit()\n",
    "\n",
    "if __name__ == \"__main__\":\n",
    "    main()"
   ]
